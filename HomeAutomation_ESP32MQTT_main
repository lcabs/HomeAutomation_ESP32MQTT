#include "WiFi.h"


void setup(){
  Serial.begin(115200);
  Serial.println("Serial iniciada.");
  initWifi();

}

void loop(){


}


void initWiFi() {
							
const char* ssid = "EG8115V5";			// Replace with your network credentials (STATION)
const char* password = "alpha147147";

	WiFi.mode(WIFI_STA);
  WiFi.begin(ssid, password);
  Serial.print("Connecting to WiFi ..");
  while (WiFi.status() != WL_CONNECTED) {
    Serial.print('.');
    delay(1000);
  }
	Serial.print("Conectado ao IP: ");
  Serial.println(WiFi.localIP());
}
